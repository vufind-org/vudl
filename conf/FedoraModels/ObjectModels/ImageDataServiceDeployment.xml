      <wsdl:output message="this:dissemResponse"></wsdl:output>
    </wsdl:operation>
    
  </wsdl:portType>
  <wsdl:service name="ImageDataService">
    <wsdl:port binding="this:ImageDataService_http" name="ImageDataService_port">
      <http:address location="LOCAL"></http:address>
    </wsdl:port>
  </wsdl:service>
  <wsdl:binding name="ImageDataService_http" type="this:ImageDataServicePortType">
    <http:binding verb="GET"></http:binding>

    <wsdl:operation name="generateImageDerivative">
      <http:operation location="http://localhost/services/genImageDerivative.php?PID=(PID)&amp;dsID=(dsID)"></http:operation>
      <wsdl:input>
        <http:urlReplacement></http:urlReplacement>
      </wsdl:input>
      <wsdl:output>
        <mime:content type="text/xml"></mime:content>
      </wsdl:output>
    </wsdl:operation>
    
    <wsdl:operation name="generateOCR">
