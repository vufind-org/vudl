  </wsdl:types>

  <wsdl:message name="generateImageDerivativeRequest">
    <wsdl:part name="PID" type="this:PIDType"></wsdl:part>
    <wsdl:part name="dsID" type="this:dsIDType"></wsdl:part>
  </wsdl:message>
  
