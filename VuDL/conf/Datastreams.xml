<xforms:instance id="Datastreams"
                 xmlns:xforms="http://www.w3.org/2002/xforms"
                 xmlns:xxforms="http://orbeon.org/oxf/xml/xforms"
                 xmlns:ev="http://www.w3.org/2001/xml-events">
    <datastreams>
        <!-- Core -->
        <datastream dsID="THUMBNAIL" label="Thumbnail"/>
        <datastream dsID="LICENSE" label="License"/>
        <!-- Collection -->
        <datastream dsID="STRUCTMAP" label="Sort Order"/>
        <!-- Resource -->
        <datastream dsID="AGENTS" label="Agents"/>
        <datastream dsID="PROCESS-MD" label="Process MD"/>
        <!-- Data -->
        <datastream dsID="MASTER" label="Master File"/>
        <datastream dsID="MASTER-MD" label="Master MD"/>
        <!-- Image -->
        <datastream dsID="LARGE" label="Large"/>
        <datastream dsID="MEDIUM" label="Medium"/>
        <datastream dsID="OCR-DIRTY" label="OCR Dirty"/>
        <datastream dsID="OCR-EDITED" label="OCR Edited"/>
        <!-- Segment -->
        <datastream dsID="COORDINATES" label="Coordinates"/>
        <!-- Audio -->
        <datastream dsID="OGG" label=".ogg"/>
        <datastream dsID="WAV" label=".wav"/>
        <datastream dsID="MP3" label=".mp3"/>
        
    </datastreams>
</xforms:instance>